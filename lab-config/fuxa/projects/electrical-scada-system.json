  "scripts": [
    {
      "id": "power_balance_script",
      "name": "Script Équilibre Puissance",
      "description": "Script pour maintenir l'équilibre entre génération et consommation",
      "language": "javascript",
      "code": "function checkPowerBalance() {\n    var generation = gen1_power + gen2_power;\n    var consumption = load1_power + load2_power + load3_power;\n    var balance = generation - consumption;\n    \n    if (balance < -10) {\n        triggerAlarm('Déficit de puissance: ' + Math.abs(balance) + ' MW');\n    } else if (balance > 10) {\n        triggerAlarm('Excédent de puissance: ' + balance + ' MW');\n    }\n    \n    return balance;\n}",
      "enabled": true,
      "trigger": "on_data_change"
    },
    {
      "id": "frequency_control_script",
      "name": "Script Contrôle Fréquence",
      "description": "Script pour contrôler la fréquence du système",
      "language": "javascript",
      "code": "function controlFrequency() {\n    var frequency = gen1_frequency;\n    \n    if (frequency < 49.8) {\n        // Augmenter la génération\n        setTagValue('gen1_power', gen1_power + 5);\n    } else if (frequency > 50.2) {\n        // Réduire la génération\n        setTagValue('gen1_power', Math.max(0, gen1_power - 5));\n    }\n}",
      "enabled": true,
      "trigger": "on_timer",
      "interval": 5000
    }
  ]
